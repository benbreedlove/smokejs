<ul data-attribute="{attribute}"
    data-slug="{slug}"
    data-template="{template}" >
  {#list list_length=list.length}
    {@if cond="{$idx} < {list_length} - 1"}
      {#render template=template delimiter=delimiter /}
    {:else}
      {#render template=template end=end /}
    {/if}
  {/list}
</ul>
