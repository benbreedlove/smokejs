<article>
	<header>
		<h1>MoJo on <em>{title}</em></h1>
    <h5>this is a topic page</h5>
	</header>
  <ol>
    {#members}
			<li>
				<a href="{article.uri}"><img src="{master_image.uri}" alt="" title="" width="150" height="84" /></a>
				<div class="section-item">
					<h3><a href="{article.uri}">{article.metadata.title}</a></h3>
					<p class="byline">&mdash;<a href="" rel="author">{article.byline.first_name} {article.byline.last_name}</a></p>
				</div>
    {/members}
  </ol>
</article>
