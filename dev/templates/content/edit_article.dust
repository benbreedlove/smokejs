<form>
    <formfield name="metadata">
        <label for="title">Title:</label>
        <input name="title" value="{metadata.title}"></input>

        <label for="dek">Dek:</label>
        <input name="dek" value="{metadata.dek}"></input>

        <label for="master_image">Master Image</label>
        {#master_image}
        <div class="master_preview">
            {load_asset}
        </div>
        {/master_image}
    </formfield>
    <formfield name="members">
        {#members}
            <label>page {order} </label>
            <textarea>
                {load_asset}
            </textarea>
        {/members}
    </formfield>
</form>
